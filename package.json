{
  "name": "tribbb",
  "version": "1.0.0",
  "description": "",
  "main": "dist/tribbb.js",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && rollup --config && npm test",
    "clean": "rm -rf ./dist && mkdir dist",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "types": "./node_modules/.bin/dts-bundle-generator -o dist/index.d.ts src/tribbb.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsplumb/tribbb.git"
  },
  "author": "jsPlumb <hello@jsplumbtoolkit.com> (https://jsplumbtoolkit.com)",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/jsplumb/tribbb/issues"
  },
  "homepage": "https://github.com/jsplumb/tribbb#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "dts-bundle-generator": "^1.1.0",
    "jest": "^22.4.3",
    "rollup": "^0.58.2",
    "rollup-plugin-typescript2": "^0.13.0",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!shiva/)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      },
      "google": {}
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.schema.ts",
      "!src/schema/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)test.ts",
      "<rootDir>/test/**/?(*.)test.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.js$": "babel-jest"
    }
  }
}
